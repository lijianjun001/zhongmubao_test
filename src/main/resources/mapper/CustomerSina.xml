<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhongmubao.api.dao.CustomerSinaDao">
    <select id="getCustomerSinaById" resultType="CustomerSina">
        SELECT
        *
        FROM
        CustomerSina
        WHERE
        customerid = #{customerId} limit 1
    </select>
</mapper>