<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhongmubao.api.dao.ExtFailedLoginDao">
    <!-- 目的：为dao接口方法提供sql语句配置 -->

    <select id="getExtFailedLoginById" resultType="ExtFailedLogin">
        <!-- 具体的sql -->
        SELECT
        *
        FROM
        ExtFailedLogin
        WHERE
        id = #{id} limit 1
    </select>

    <select id="pagerExtFailedLoginList" resultType="ExtFailedLogin">
        SELECT
        *
        FROM
        ExtFailedLogin
        ORDER BY
        id
        LIMIT #{offset}, #{limit}
    </select>

    <insert id="insertExtFailedLogin">
        <!-- ignore 主键冲突，报错 -->
        INSERT ignore INTO ExtFailedLogin (
			     Account, 
	     Code, 
	     FailedNum, 
	     Created, 
	     Modified 
		)
        VALUES (
			     #{account}, 
	     #{code}, 
	     #{failedNum}, 
	     #{created}, 
	     #{modified} 
		)
    </insert>
</mapper>